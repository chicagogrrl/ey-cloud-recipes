Sidekiq.configure_server do |config|
  config.redis = {
    <% if @namespace %>
     namespace: <%=@namespace%>,
    <%end %>
     url: <%=@url%>
  }
end
Sidekiq.configure_client do |config|
  config.redis = {
    <% if @namespace %>
     namespace: <%=@namespace%>,
    <%end %>
     url: <%=@url%>
  }
end
